{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "jsx": "react",
    "rootDir": "src",
    "baseUrl": "src",
    "paths":{
      "@/*":["*"]
    },
    // "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
  },
  "include": ["src"]
}
